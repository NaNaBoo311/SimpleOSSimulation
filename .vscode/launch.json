{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug OS (with args)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/os",
      "args": ["os_1_mlq_paging"],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
